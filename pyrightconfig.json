{
    "include": [
        "ecommerce-backend"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__"
    ],
    "pythonVersion": "3.13",
    "pythonPlatform": "Darwin",
    "executionEnvironments": [
        {
            "root": "ecommerce-backend",
            "pythonVersion": "3.13",
            "pythonPlatform": "Darwin",
            "extraPaths": [
                "ecommerce-backend"
            ]
        }
